{
  "name": "kalori-makanan-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "serve": "vite preview --port 8888",
    "dev:build": "npm run build && npm run serve",
    "netlify:dev": "npm run build && netlify dev --dir dist",
    "netlify:build": "npm run build",
    "netlify:deploy": "netlify deploy --prod",
    "test:keep-alive": "curl -s http://localhost:8888/.netlify/functions/keep-alive | jq ."
  },
  "dependencies": {
    "@libsql/client": "^0.15.9",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.3.4"
  },
  "devDependencies": {
    "@netlify/functions": "^4.1.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
